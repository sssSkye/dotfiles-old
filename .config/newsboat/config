auto-reload yes
browser firefox

macro w set browser "mpv --opengl-early-flush=no %u" ; open-in-browser ; set browser firefox
macro v set browser "youtube-dl --add-metadata -ic" ; open-in-browser ; set browser firefox

bind-key ; macro-prefix
bind-key j next
bind-key k prev
bind-key J next-feed
bind-key K prev-feed
bind-key j down article
bind-key k up article
bind-key J next article
bind-key K prev article
